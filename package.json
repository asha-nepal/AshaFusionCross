{
  "name": "AshaFusionCross",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "electron": "electron build/main.js",
    "electron:dev": "NODE_ENV=development electron build/main.js",
    "test": "jest",
    "flow": "flow",
    "lint": "eslint src",
    "build": "NODE_ENV=production webpack --progress --colors --config webpack.config.web.js",
    "build:demo": "COUCH_HOST='\"demo.asha-np.org\"' npm run build",
    "electron:build:dev": "NODE_ENV=development webpack --progress --colors --config webpack.config.electron.dev.js",
    "electron:build:prod": "NODE_ENV=production webpack --progress --colors --config webpack.config.electron.prod.js",
    "electron:pack:mac": "electron-packager build asha-fusion --overwrite --asar --platform=darwin --arch=all --icon=assets/icons/mac/asha.icns --out=electron-build",
    "electron:pack:win": "electron-packager build asha-fusion --overwrite --asar --platform=win32 --arch=all --icon=assets/icons/win/asha.ico --out=electron-build",
    "electron:pack:linux": "electron-packager build asha-fusion --overwrite --asar --platform=linux --arch=all --icon=assets/icons/png/1024x1024.png --out=electron-build",
    "electron:pack:all": "npm run electron:pack:mac && npm run electron:pack:win && npm run electron:pack:linux",
    "server": "if-env NODE_ENV=production && npm run server:prod || npm run server:dev",
    "server:dev": "webpack-dev-server --config webpack.config.web.js",
    "server:prod": "node server.js"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "csv-stringify": "^1.0.4",
    "express": "^4.14.0",
    "if-env": "^1.0.0",
    "randomstring-promise": "0.0.1",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-data-grid": "^2.0.4",
    "react-dates": "^7.0.1",
    "react-native": "^0.42.3",
    "react-native-material-kit": "^0.4.1",
    "react-native-randombytes": "^2.2.0",
    "react-native-router-flux": "^3.38.0",
    "react-native-vector-icons": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-jest": "19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-minimal": "^2.1.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-runtime": "^6.9.0",
    "balloon-css": "^0.3.0",
    "blob-polyfill": "^1.0.20150320",
    "bulma": "^0.2.3",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "ejs-loader": "^0.3.0",
    "electron": "^1.6.6",
    "electron-packager": "^8.6.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-flowtype": "^2.3.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.38.0",
    "font-awesome": "^4.6.3",
    "html-loader": "^0.4.3",
    "html-webpack-exclude-assets-plugin": "0.0.3",
    "html-webpack-include-assets-plugin": "0.0.3",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "jest": "19.0.2",
    "json-loader": "^0.5.4",
    "lodash.get": "^4.4.2",
    "lodash.sortby": "^4.7.0",
    "lodash.topath": "^4.5.2",
    "lodash.uniqby": "^4.7.0",
    "mathjs": "^3.3.0",
    "mockdate": "^1.0.4",
    "moment": "^2.13.0",
    "node-sass": "^3.8.0",
    "pouchdb": "^6.0.6",
    "pouchdb-authentication": "^0.5.3",
    "react-addons-perf": "^15.3.1",
    "react-addons-test-utils": "^15.1.0",
    "react-autosuggest": "^5.1.0",
    "react-dom": "^15.0.2",
    "react-native-drawer": "^2.3.0",
    "react-redux": "^4.4.5",
    "react-redux-form": "^1.0.13",
    "react-router": "^2.4.1",
    "recharts": "^0.13.4",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.4",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^2.6.1",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "jest": {
    "automock": true,
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/react-redux-form/"
    ],
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^.+\\.(gif|png|jpg|jpeg|html)$": "<rootDir>/test/fileMock.js",
      "randomstring": "randomstring-promise"
    }
  }
}
