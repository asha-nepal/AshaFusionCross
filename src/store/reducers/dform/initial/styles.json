{
  "patient": [
    {
      "style": [
        {
          "field": "number",
          "label": "No.",
          "style": {
            "width": 200
          },
          "valueSetters": [
            {
              "label": "Set next No.",
              "optionKey": "nextPatientNumber"
            }
          ]
        },
        {
          "field": "name",
          "label": "Name",
          "class": "textinput",
          "required": true
        },
        {
          "field": "age",
          "label": "Age",
          "class": "textunitinput",
          "units": [
            "years",
            "months"
          ],
          "precision": 0
        },
        {
          "field": "district",
          "label": "District",
          "class": "textinput"
        },
        {
          "class": "block",
          "children": [
            {
              "field": "municipality_vdc.name",
              "label": "Municipality/VDC",
              "class": "textinput"
            },
            {
              "field": "municipality_vdc.no",
              "label": "Municipality/VDC No.",
              "class": "textinput",
              "type": "number"
            }
          ]
        },
        {
          "field": "area",
          "label": "Area",
          "class": "textinput"
        },
        {
          "field": "contact_number",
          "label": "Contact Number",
          "class": "textinput"
        },
        {
          "field": "blood_type",
          "label": "Blood Type",
          "class": "select",
          "options": [
            { "id": "A", "label": "A" },
            { "id": "B", "label": "B" },
            { "id": "O", "label": "O" },
            { "id": "AB", "label": "AB" }
          ]
        },
        {
          "class": "block",
          "children": [
            {
              "field": "husband.name",
              "label": "Husband's Name",
              "class": "textinput"
            },
            {
              "field": "husband.contact_number",
              "label": "Husband's Contact Number",
              "class": "textinput"
            }
          ]
        }
      ]
    }
  ],
  "record": [
    {
      "id": "anc",
      "label": "ANC",
      "style": [
        {
          "field": "prev_pregnancy",
          "label": "Previous history of Pregnancy",
          "class": "subformlist",
          "ditto": true,
          "fields": [
            {
              "field": "living",
              "label": "Living",
              "class": "check"
            },
            {
              "field": "born_dead",
              "label": "Born Dead",
              "class": "check",
              "show": "!living"
            },
            {
              "field": "born_within_36_weeks",
              "label": "Born less than 36 weeks",
              "class": "check"
            },
            {
              "field": "twins",
              "label": "Twins",
              "class": "check"
            },
            {
              "field": "abortion",
              "label": "Abortion",
              "class": "check"
            },
            {
              "field": "gender",
              "label": "Gender",
              "class": "radio",
              "options": [
                {
                  "id": 1,
                  "label": "Male"
                },
                {
                  "id": 2,
                  "label": "Female"
                }
              ]
            },
            {
              "field": "age",
              "label": "Current Age",
              "class": "textunitinput",
              "units": [
                "years",
                "months"
              ],
              "precision": 0
            },
            {
              "field": "pregnancy_complication",
              "label": "Pregnancy Complication",
              "class": "check"
            },
            {
              "field": "pregnancy_complication_detail",
              "class": "textarea",
              "show": "pregnancy_complication"
            },
            {
              "field": "",
              "label": "Mode of Delivery",
              "class": "radio",
              "options": [
                {
                  "id": "normal",
                  "label": "Normal"
                },
                {
                  "id": "cs",
                  "label": "CS"
                }
              ]
            }
          ]
        },
        {
          "label": "Pregnancy Vaccination",
          "class": "block",
          "children": [
            {
              "label": "T.D Vaccine Registerd Number",
              "field": "td_vaccine.regd_no",
              "class": "textinput",
              "type": "number"
            },
            {
              "label": "1st Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.1st.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.1st.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.1st.taken"
                }
              ]
            },
            {
              "label": "2nd Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.2nd.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.2nd.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.2nd.taken"
                }
              ]
            },
            {
              "label": "2+ Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.2+.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.2+.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.2+.taken"
                }
              ]
            }
          ]
        },
        {
          "class": "block",
          "children": [
            {
              "label": "Last Menstrual Date",
              "field": "last_menstrual_date",
              "class": "textinput"
            },
            {
              "label": "Estimated Date of Delivery",
              "field": "estimated_date_of_delivery",
              "class": "textinput"
            }
          ]
        },
        {
          "label": "Pregnancy Examination",
          "class": "subformlist",
          "field": "pregnancy_exam",
          "fields": [
            {
              "label": "Months",
              "field": "months",
              "class": "textinput",
              "type": "number"
            },
            {
              "label": "Date",
              "field": "date",
              "class": "textinput"
            },
            {
              "label": "Weight",
              "field": "weight",
              "class": "textunitinput",
              "type": "number",
              "precision": 1,
              "units": [
                "kg",
                "lb"
              ]
            },
            {
              "label": "Anemia",
              "field": "anemia",
              "class": "check"
            },
            {
              "label": "Edema",
              "field": "edema",
              "class": "check"
            },
            {
              "label": "Blood pressure",
              "field": "blood_pressure",
              "class": "textinput",
              "type": "number",
              "suffix": "mmHg",
              "min": 1,
              "precision": 0
            },
            {
              "label": "Pregnancy in weeks",
              "field": "pregnancy_in_weeks",
              "class": "textinput"
            },
            {
              "label": "Size of uterus",
              "field": "size_of_uterus",
              "class": "textunitinput",
              "type": "number",
              "units": [
                "cm",
                "in"
              ]
            },
            {
              "label": "Foetus",
              "field": "foetus.found",
              "class": "check"
            },
            {
              "label": "Child Heart Rate",
              "field": "foetus.child_heart_rate",
              "class": "textinput",
              "type": "number",
              "show": "foetus.found"
            },
            {
              "label": "Any problem",
              "field": "any_problem",
              "class": "textarea"
            },
            {
              "label": "Next Consultation Date",
              "field": "next_consultation_date",
              "class": "textinput"
            },
            {
              "label": "Number of Iron Tablets",
              "field": "number_of_iron_tablets",
              "class": "textinput",
              "type": "number",
              "min": 0,
              "precision": 0
            },
            {
              "label": "Alben",
              "field": "alben",
              "class": "textinput"
            },
            {
              "label": "T.D",
              "field": "td_vaccine",
              "class": "check"
            },
            {
              "label": "Health Worker Name",
              "field": "health_worker_name",
              "class": "textinput"
            },
            {
              "label": "Name of Hospital",
              "field": "hospital_name",
              "class": "textinput"
            }
          ]
        }
      ]
    }
  ]
}
