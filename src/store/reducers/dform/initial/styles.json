{
  "patient": [
    {
      "style": [
        {
          "field": "number",
          "label": "No.",
          "style": {
            "width": 200
          },
          "valueSetters": [
            {
              "label": "Set next No.",
              "optionKey": "nextPatientNumber"
            }
          ]
        },
        {
          "field": "name",
          "label": "Name",
          "class": "textinput",
          "required": true
        },
        {
          "field": "age",
          "label": "Age",
          "class": "textunitinput",
          "units": [
            "years",
            "months"
          ],
          "precision": 0
        },
        {
          "field": "ht",
          "label": "Ht.",
          "class": "textinput"
        },
        {
          "field": "district",
          "label": "District",
          "class": "textinput"
        },
        {
          "class": "block",
          "children": [
            {
              "field": "municipality_vdc.name",
              "label": "Municipality/VDC",
              "class": "textinput"
            },
            {
              "field": "municipality_vdc.no",
              "label": "Municipality/VDC No.",
              "class": "textinput",
              "type": "number"
            }
          ]
        },
        {
          "field": "area",
          "label": "Area",
          "class": "textinput"
        },
        {
          "field": "contact_number",
          "label": "Contact Number",
          "class": "textinput"
        },
        {
          "field": "blood_type",
          "label": "Blood Type",
          "class": "select",
          "options": [
            { "id": "A", "label": "A" },
            { "id": "B", "label": "B" },
            { "id": "O", "label": "O" },
            { "id": "AB", "label": "AB" }
          ]
        },
        {
          "field": "number_of_delivery",
          "label": "Number of delivery",
          "class": "textinput",
          "type": "number"
        },
        {
          "field": "husband_name",
          "label": "Husband's Name",
          "class": "textinput"
        }
      ]
    }
  ],
  "record": [
    {
      "id": "anc",
      "label": "ANC",
      "style": [
        {
          "class": "block",
          "children": [
            {
              "label": "Last Menstrual Date",
              "field": "last_menstrual_date",
              "class": "textinput"
            },
            {
              "label": "Estimated Date of Delivery",
              "field": "estimated_date_of_delivery",
              "class": "textinput"
            }
          ]
        },
        {
          "label": "Previous Pregnancy Details",
          "class": "accordion",
          "widthaligned": true,
          "children": [
            {
              "field": "prev_pregnancy",
              "label": "Previous history of Pregnancy",
              "class": "subformlist",
              "ditto": true,
              "autoAdd": false,
              "fields": [
                {
                  "field": "living",
                  "label": "Living",
                  "class": "check"
                },
                {
                  "field": "born_dead",
                  "label": "Born Dead",
                  "class": "check",
                  "show": "!living"
                },
                {
                  "field": "born_before_37_weeks",
                  "label": "Born before 37 weeks",
                  "class": "check"
                },
                {
                  "field": "twins",
                  "label": "Twins",
                  "class": "check"
                },
                {
                  "field": "abortion",
                  "label": "Abortion",
                  "class": "check"
                },
                {
                  "field": "gender",
                  "label": "Gender",
                  "class": "radio",
                  "show": "living",
                  "options": [
                    {
                      "id": 1,
                      "label": "Male"
                    },
                    {
                      "id": 2,
                      "label": "Female"
                    }
                  ]
                },
                {
                  "field": "age",
                  "label": "Current Age",
                  "class": "textunitinput",
                  "show": "living",
                  "units": [
                    "years",
                    "months"
                  ],
                  "precision": 0
                },
                {
                  "field": "pregnancy_complication",
                  "label": "Pregnancy Complication",
                  "class": "check"
                },
                {
                  "field": "pregnancy_complication_detail",
                  "class": "textarea",
                  "show": "pregnancy_complication",
                  "shrink": true
                },
                {
                  "field": "type_of_delivery",
                  "label": "Type of Delivery",
                  "class": "radio",
                  "options": [
                    {
                      "id": "normal",
                      "label": "Normal"
                    },
                    {
                      "id": "cs",
                      "label": "CS"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "label": "Pregnancy Vaccination",
          "class": "accordion",
          "children": [
            {
              "label": "T.D Vaccine Registerd Number",
              "field": "td_vaccine.regd_no",
              "class": "textinput",
              "type": "number"
            },
            {
              "label": "1st Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.first.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.first.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.first.taken"
                }
              ]
            },
            {
              "label": "2nd Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.second.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.second.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.second.taken"
                }
              ]
            },
            {
              "label": "2+ Vaccination",
              "class": "block",
              "children": [
                {
                  "field": "td_vaccine.after_second.taken",
                  "class": "check"
                },
                {
                  "field": "td_vaccine.after_second.date",
                  "class": "textinput",
                  "placeholder": "Date?",
                  "show": "td_vaccine.after_second.taken"
                }
              ]
            }
          ]
        },
        {
          "label": "Details of Pregnancy Checkup",
          "class": "accordion",
          "widthaligned": true,
          "children": [
            {
              "label": "Pregnancy Examination",
              "class": "subformlist",
              "field": "pregnancy_exam",
              "ditto": true,
              "autoAdd": false,
              "fields": [
                {
                  "label": "Months",
                  "field": "months",
                  "class": "textinput",
                  "type": "number"
                },
                {
                  "label": "Date",
                  "field": "date",
                  "class": "textinput"
                },
                {
                  "label": "Weight",
                  "field": "weight",
                  "class": "textunitinput",
                  "type": "number",
                  "precision": 1,
                  "units": [
                    "kg",
                    "lb"
                  ]
                },
                {
                  "label": "Anemia",
                  "field": "anemia",
                  "class": "check"
                },
                {
                  "label": "Edema (Hand)",
                  "field": "edema.hand",
                  "class": "check"
                },
                {
                  "label": "Edema (Face)",
                  "field": "edema.face",
                  "class": "check"
                },
                {
                  "label": "Blood Pressure (s)",
                  "field": "bp.s",
                  "class": "textinput",
                  "placeholder": "sBP",
                  "type": "number",
                  "style": {
                    "width": 60
                  },
                  "suffix": "mmHg",
                  "min": 1,
                  "precision": 0,
                  "alerts": [
                    {
                      "type": "warning",
                      "label": "Low",
                      "range": [
                        null,
                        100
                      ]
                    },
                    {
                      "type": "success",
                      "label": "Normal",
                      "range": [
                        100,
                        140
                      ]
                    },
                    {
                      "type": "warning",
                      "label": "High",
                      "range": [
                        140,
                        180
                      ]
                    },
                    {
                      "type": "danger",
                      "label": "Alert",
                      "range": [
                        180,
                        null
                      ]
                    }
                  ]
                },
                {
                  "label": "Blood Pressure (d)",
                  "field": "bp.d",
                  "class": "textinput",
                  "placeholder": "dBP",
                  "type": "number",
                  "style": {
                    "width": 60
                  },
                  "suffix": "mmHg",
                  "min": 1,
                  "precision": 0,
                  "alerts": [
                    {
                      "type": "warning",
                      "label": "Low",
                      "range": [
                        null,
                        60
                      ]
                    },
                    {
                      "type": "success",
                      "label": "Normal",
                      "range": [
                        60,
                        90
                      ]
                    },
                    {
                      "type": "warning",
                      "label": "High",
                      "range": [
                        90,
                        110
                      ]
                    },
                    {
                      "type": "danger",
                      "label": "Alert",
                      "range": [
                        110,
                        null
                      ]
                    }
                  ]
                },
                {
                  "label": "Pregnancy in weeks",
                  "field": "pregnancy_in_weeks",
                  "class": "textinput"
                },
                {
                  "label": "Size of uterus",
                  "field": "size_of_uterus",
                  "class": "textunitinput",
                  "type": "number",
                  "units": [
                    "cm",
                    "in"
                  ]
                },
                {
                  "label": "Foetus",
                  "field": "foetus.found",
                  "class": "check"
                },
                {
                  "label": "Child Heart Rate",
                  "field": "foetus.child_heart_rate",
                  "class": "textinput",
                  "type": "number",
                  "show": "foetus.found"
                },
                {
                  "label": "Any problem",
                  "field": "any_problem",
                  "class": "textarea",
                  "shrink": true
                },
                {
                  "label": "Next Consultation Date",
                  "field": "next_consultation_date",
                  "class": "textinput"
                },
                {
                  "label": "Number of Iron Tablets",
                  "field": "number_of_iron_tablets",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "precision": 0
                },
                {
                  "label": "Alben",
                  "field": "alben",
                  "class": "textinput"
                },
                {
                  "label": "T.D",
                  "field": "td_vaccine",
                  "class": "check"
                },
                {
                  "label": "Health Worker Name",
                  "field": "health_worker_name",
                  "class": "textinput"
                },
                {
                  "label": "Name of Hospital",
                  "field": "hospital_name",
                  "class": "textinput"
                }
              ]
            }
          ]
        },
        {
          "label": "Health Worker, who prepared the card",
          "class": "accordion",
          "children": [
            {
              "class": "block",
              "children": [
                {
                  "label": "Name",
                  "field": "health_worker.name",
                  "class": "textinput"
                },
                {
                  "label": "Surname",
                  "field": "health_worker.surname",
                  "class": "textinput"
                },
                {
                  "label": "Designation",
                  "field": "health_worker.designation",
                  "class": "textinput"
                },
                {
                  "label": "Date",
                  "field": "health_worker.date",
                  "class": "textinput"
                }
              ]
            },
            {
              "class": "block",
              "children": [
                {
                  "label": "Hospital Contact Number",
                  "field": "hospital_contact_number",
                  "class": "textinput"
                },
                {
                  "label": "Ambulance Contact Number",
                  "field": "ambulance_contact_number",
                  "class": "textinput"
                }
              ]
            }
          ]
        },
        {
          "label": "Laboratory Examination",
          "class": "accordion",
          "widthaligned": true,
          "children": [
            {
              "class": "subformlist",
              "field": "lab_exam",
              "ditto": true,
              "autoAdd": false,
              "fields": [
                {
                  "label": "Date",
                  "field": "date",
                  "class": "textinput",
                  "style": {
                    "width": 100
                  }
                },
                {
                  "label": "Hb",
                  "field": "hb",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "suffix": "g/dl",
                  "style": {
                    "width": 80
                  },
                  "alerts": [
                    {
                      "type": "warning",
                      "label": "Low",
                      "range": [ null, 12 ]
                    },
                    {
                      "type": "success",
                      "label": "Normal",
                      "range": [ 12, 18 ]
                    },
                    {
                      "type": "warning",
                      "label": "High",
                      "range": [ 18, null]
                    }
                  ]
                },
                {
                  "label": "Albumin",
                  "field": "albumin",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "suffix": "gm%",
                  "style": {
                    "width": 80
                  },
                  "alerts": [
                    {
                      "type": "warning",
                      "label": "Low",
                      "range": [ null, 3.5 ]
                    },
                    {
                      "type": "success",
                      "label": "Normal",
                      "range": [ 3.5, 5.5 ]
                    },
                    {
                      "type": "warning",
                      "label": "High",
                      "range": [ 5.5, null]
                    }
                  ]
                },
                {
                  "label": "Urine (Protein)",
                  "field": "urine.protein",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "Urine (Sugar)",
                  "field": "urine.sugar",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "Blood Sugar",
                  "field": "blood_sugar",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "suffix": "mg/dL",
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "HBsAg",
                  "field": "hbsag",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "VDRL",
                  "field": "vdrl",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "Retro Virus",
                  "field": "retro_virus",
                  "class": "textinput",
                  "type": "number",
                  "min": 0,
                  "style": {
                    "width": 80
                  }
                },
                {
                  "label": "Other",
                  "field": "other",
                  "class": "textarea",
                  "shrink": true
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
