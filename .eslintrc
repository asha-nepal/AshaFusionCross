{
    "parser": "babel-eslint",
    "plugins": [
        "flowtype",
        "flow-vars"
    ],
    "extends": "airbnb",
    "rules": {
        "no-underscore-dangle": 0,
        "no-constant-condition": [2, { "checkLoops": false }],
        "flow-vars/define-flow-type": 1,
        "flow-vars/use-flow-type": 1
    }
}
